    <script>
        let id_instituicao = '<%= user.intituicao_responsavel.id %>'


        Vue.createApp({
            data() {
                return {
                    nome: '',
                    username: "",
                    sobre: "",
                    email: "",
                    contacto: "",
                    data: "",
                    sexo: "M",
                    morada: "",
                    arquivo: [],
                    tecnico: {

                    }

                }
            },
            methods: {

                processFile(event) {
                    this.arquivo = event.target.files
                    console.log(this.arquivo)
                },


                selet(tecnico) {

                    this.tecnico = tecnico
                    console.log(tecnico)

                },
                selectFoto(foto) {
                    //  this.user=user

                    let fotos = []
                    fotos.push(foto.id)

                    this.tecnico.foto.map((fot) => {

                        if (foto.id != fot.id) {
                            fotos.push(fot.id)
                        }
                    })

                    axios
                        .put('/api/instituicao/tecnicos/' + this.tecnico.id, {
                            data: {

                                foto: fotos,


                            },

                        })
                        .then(({
                            data
                        }) => {

                            swal('Edição de Técnico', 'Ação feita com sucesso!!', 'success').then(() => {
                                location.reload()
                            });

                            console.log(data)
                        })

                    console.log(fotos)
                },
                selectFotoEliminar(foto) {

                    console.log(foto)


                    swal({
                            title: 'Are you sure you want to delete?',
                            text: 'Yes or No?',
                            icon: 'warning',
                            buttons: true,
                            dangerMode: true,
                        })
                        .then((willDelete) => {
                            if (willDelete) {
                                axios.delete(`/api/upload/files/${foto.id}`)
                                    .then(response => {
                                        console.log(response.data);
                                        swal('Good Job', 'Photo deleted successfully!', 'success').then(() => {
                                            location.reload()

                                        })


                                    }).catch(function(error) {
                                        console.log(error);
                                        swal('Gestão do técnico', error.response.data.error.message, 'warning').then(() => {
                                            //location.reload()
                                        });

                                    });
                            } else {
                                swal('Photo not deleted successfully!');
                            }
                        });


                },

                register(event) {
                    console.log(this.email, this.username, this.password, )

                    let formData = new FormData();

                    Array.from(this.arquivo).forEach(image => {

                        formData.append('files', image);
                        console.log(formData, image)
                    });

                    event.preventDefault();



                    if (this.arquivo.length) {
                        axios.post(`/api/upload/`, formData)
                            .then(async res1 => {

                                axios
                                    .post('/api/auth/register', {

                                        email: this.email,
                                        username: this.username,
                                        nome: this.nome,
                                        morada: this.morada,
                                        contacto: this.contacto,
                                        data: this.data,
                                        sexo: this.sexo,
                                        descricao: this.sobre,
                                        foto: res1.data[0].id,
                                        intituicao_tecnico: [parseInt(id_instituicao)]


                                    })
                                    .then(({
                                        data
                                    }) => {
                                        swal('Adição de Técnico', 'Ação feita com sucesso!!', 'success').then(() => {
                                            location.reload()
                                        });

                                    })
                                    .catch(error => {
                                        // Handle error.
                                        //err1.innerHTML=""
                                        //window.location.href="/auth/login"
                                        console.log('An error occurred:', error.response);
                                        swal('Gestão do técnico', error.response.data.error.message, 'warning').then(() => {
                                            //location.reload()
                                        });

                                        if (error.response.status == '400') {


                                        }
                                    });

                                // not1("Arquivo carregado")
                            }).catch(error => {

                                axios
                                    .post('/api/auth/register', {

                                        email: this.email,
                                        username: this.username,
                                        nome: this.nome,
                                        morada: this.morada,
                                        contacto: this.contacto,
                                        data: this.data,
                                        sexo: this.sexo,
                                        descricao: this.sobre,
                                        intituicao_tecnico: [parseInt(id_instituicao)]


                                    })
                                    .then(({
                                        data
                                    }) => {
                                        swal('Adição de Técnico', 'Ação feita com sucesso!!', 'success').then(() => {
                                            location.reload()
                                        });

                                    })
                                    .catch(error => {
                                        // Handle error.
                                        //err1.innerHTML=""
                                        //window.location.href="/auth/login"
                                        console.log('An error occurred:', error.response);

                                        swal('Gestão do técnico', error.response.data.error.message, 'warning').then(() => {
                                            //location.reload()
                                        });

                                        if (error.response.status == '400') {


                                        }
                                    });

                            });
                    } else {
                        axios
                            .post('/api/auth/register', {

                                email: this.email,
                                username: this.username,
                                nome: this.nome,
                                morada: this.morada,
                                contacto: this.contacto,
                                data: this.data,
                                sexo: this.sexo,
                                descricao: this.sobre,
                                intituicao_tecnico: [parseInt(id_instituicao)]


                            })
                            .then(({
                                data
                            }) => {
                                swal('Adição de Técnico', 'Ação feita com sucesso!!', 'success').then(() => {
                                    location.reload()
                                });

                            })
                            .catch(error => {
                                // Handle error.
                                //err1.innerHTML=""
                                //window.location.href="/auth/login"
                                console.log('An error occurred:', error.response);

                                if (error.response.status == '400') {;

                                }

                                swal('Gestão do técnico', error.response.data.error.message, 'warning').then(() => {
                                    //location.reload()
                                });
                            });







                    }





                },
