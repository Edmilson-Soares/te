                    swal({
                            title: 'Tens Certeza que queres eliminar?',
                            text: '',
                            icon: 'warning',
                            buttons: true,
                            dangerMode: true,
                        })
                        .then((willDelete) => {
                            if (willDelete) {

                                axios
                                    .delete('/api/users/' + this.tecnico.id)
